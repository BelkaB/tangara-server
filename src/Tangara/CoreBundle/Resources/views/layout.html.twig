{% extends "::base.html.twig" %}
{% block stylesheets %}
    {% include "TangaraCoreBundle:Main:stylesheets.html.twig" %}
{% endblock %}
{% block javascripts %}
    {% include 'TangaraCoreBundle:Main:javascript.html.twig' %}
{% endblock %}
{% block body %}
    {% block navbar %}
        {% include 'TangaraCoreBundle:Main:navbar.html.twig' %}
    {% endblock %}
    {% block content %}
        {% if contentTemplate %}
        <div id="content">
            {% include contentTemplate %}
        </div>
        {% else %}
        <div id="content" class="hide-at-startup">
        </div>
        {% endif %}
    {% endblock %}
    <iframe id="local-frame" src="{{ tangarajs }}" frameborder="0" scrolling="no"></iframe>
{% endblock %}

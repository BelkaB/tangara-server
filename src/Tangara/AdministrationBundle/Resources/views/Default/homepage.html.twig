<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
    <head>
        {% block header %}
            {% include "TangaraAdministrationBundle:Default:header.html.twig" %}
            {% block header_stylesheets %}
                {% include "TangaraAdministrationBundle:Default:header_stylesheets.html.twig" %}
            {% endblock %}
        {% endblock %}
    </head>

    <body>
        <!--[if lt IE 7]>
            <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://www.mozilla.org/firefox/">Upgrade your browser</a> to improve your experience.</p>
        <![endif]-->

        {% block body %}
            {% block navbar %}
                {% include 'TangaraAdministrationBundle:Default:navbar.html.twig' %}
            {% endblock %}
        <div class="tangara" style="background-image: {{ asset('bundles/tangaraadministration/img/tangara_retina.png') }};">

            {% block jumbotron %}
                {% include 'TangaraAdministrationBundle:Default:jumbotron.html.twig' %}
            {% endblock %}

            {% block main_container %}
            <div class="container">
                    {% block container_content %}
                      {% include 'TangaraAdministrationBundle:Default:container_content.html.twig' %}
                    {% endblock %}

                    {% block container_specifications %}
                      {#{% include 'TangaraAdministrationBundle:Default:container_specifications.html.twig' %}#}
                    {% endblock %}

                    {% block container_footer%}
                      {% include 'TangaraAdministrationBundle:Default:container_footer.html.twig' %}
                    {% endblock %}
            </div> 
            {% endblock %}
        </div>
        <!-- /container -->
            {% block javascripts %}
                {% include 'TangaraAdministrationBundle:Default:javascript.html.twig' %}
            {% endblock %}
        {% endblock %}
    </body>
</html>

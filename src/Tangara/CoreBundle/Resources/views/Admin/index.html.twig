<div class="container">
    <div class="row">
        <div class="col-md-4">
            <div class="col-md-6">
                <h1>Members</h1>
                <ul>
                    {% for user in users %}
                        <a href="#"><button class="form-control"> {{  user.username|e }} <button></a>
                    {% endfor %}
                </ul>
            </div>
        </div>
        <div class="col-md-8">
            <h1>Projects</h1>
            <ul>
                
                {% for key, project in projects %}
                    <div class="col-md-6"> {{ project.name }} </div> 
            </ul>
            <ul>
                    <div class="col-md-6"> {{ project.owner }} </div>
                {% endfor %}
            <ul>
        </div>    
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-md-6">
            <div class="col-md-6">
                    <h2>{{ 'admin.members'|trans }}</h2>
                    <br>
                    <ul>
                        {% for user in users %}
                            <a href="{{ path('tangara_user_profile_show', {'user_id': user.id}) }}"><button class="form-control"> {{  user.username|e }} </button></a>
                                            <br>

                        {% endfor %}
                    </ul>
            </div>
        </div>
        <div class="col-md-6">
            <table cellspacing="30"> 
                    <h2>{{ 'admin.projects'|trans }}</h2>
                    <tr> 
                        <th>{{ 'admin.project'|trans }}</th>
                        <th><p>.     .</p></th>
                        <th>{{ 'admin.owner'|trans }}</th> 
                    </tr> 
                    {% for key, project in projects %}
                    <tr>
                        <td>
                            <a href="{{ path('tangara_project_show', {'projectId': project.id}) }}"> {{ project.name }}</a> 
                        </td>
                        <td>
                            <p> ->  </p>
                        </td>
                        <td>
                         <a href="{{ path('tangara_user_profile_show', {'user_id': project.owner.id}) }}">{{ project.owner }}</a>
                            
                        </td>
                    </tr>
                    {% endfor %}
            </table>
        </div>
    </div>
</div>